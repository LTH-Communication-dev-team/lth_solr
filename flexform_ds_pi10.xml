<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>General</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <program>
                        <TCEforms>
                           <label>Round</label>
                           <config>
                            <type>select</type>
                            <maxitems>1</maxitems>
                            <size>10</size>
                            <itemsProcFunc>user_sampleflex_addFieldsToFlexForm->getProgramsStat</itemsProcFunc>
                            <selectedListStyle>width:250px</selectedListStyle>
                            <itemListStyle>width:250px</itemListStyle>
                        </config>
                       </TCEforms>
                    </program>
                    <round>
                        <TCEforms>
                           <label>Program</label>
                           <config>
                            <type>select</type>
                            <maxitems>1</maxitems>
                            <size>10</size>
                            <itemsProcFunc>user_sampleflex_addFieldsToFlexForm->getRoundsStat</itemsProcFunc>
                            <selectedListStyle>width:250px</selectedListStyle>
                            <itemListStyle>width:250px</itemListStyle>
                        </config>
                       </TCEforms>
                    </round>
                    <insideInfobox>
                        <TCEforms>
                           <label>Put inside Infobox</label>
                           <onChange>reload</onChange>
                           <config>
                            <type>check</type>
                          </config> 
                        </TCEforms>
                    </insideInfobox>
                    <backgroundcolor>
                        <TCEforms>
                            <label>LLL:EXT:lth_package/Resources/Private/Language/backend.xlf:infobox.backgroundcolor</label>
                            <config>
                                <type>select</type>
                                <default>sky.gif</default>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0"></numIndex>
                                    </numIndex>
                                </items>
                                <fileFolder>EXT:lth_package/Resources/Public/Icons/SysColors/</fileFolder>
                                <fileFolder_extList>png,jpg,jpeg,gif</fileFolder_extList>
                                <fileFolder_recursions>0</fileFolder_recursions>
                                <size>1</size>
                                <minitems>0</minitems>
                                <maxitems>1</maxitems>
                                <selicon_cols>8</selicon_cols>
                            </config>
                        </TCEforms>
                    </backgroundcolor>
                    <fixColorBoxes>
                        <TCEforms>
                            <config>
                                <type>user</type>
                                <userFunc>user_lthpackageflex_addFieldsToFlexForm->fixColorBoxes</userFunc>
                            </config>
                        </TCEforms>
                    </fixColorBoxes>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>