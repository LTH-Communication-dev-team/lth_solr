<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
    <sDEF>
        <ROOT>
            <TCEforms>
                <sheetTitle>General</sheetTitle>
            </TCEforms>
            <type>array</type>
            <el>
                <init>
                    <TCEforms>
                        <config>
                            <type>user</type>
                            <userFunc>user_sampleflex_addFieldsToFlexForm->init</userFunc>
                        </config>
                    </TCEforms>
                </init>
                <display>
                    <TCEforms>
                        <label>What to display</label>
                        <config>
                            <type>radio</type>
                            <default>list</default>
                            <items type="array">
                                <numIndex index="0" type="array">
                                    <numIndex index="0">Publication list</numIndex>
                                    <numIndex index="1">list</numIndex>
                                </numIndex>
                                <numIndex index="1" type="array">
                                    <numIndex index="0">Tag cloud</numIndex>
                                    <numIndex index="1">tagcloud</numIndex>
                                </numIndex>
                            </items>
                        </config>
                    </TCEforms>
                </display>
            
                <fe_groups>
                    <TCEforms type="array">
                        <label>Välj en eller flera användargrupper</label>
                        <config>
                            <type>select</type>
                            <size>10</size>
                            <maxitems>100</maxitems>
                            <items type="array">
                                <numIndex index="0" type="array">
                                    <numIndex index="0"></numIndex>
                                    <numIndex index="1"></numIndex>
                                </numIndex>
                            </items>
                            <foreign_table>fe_groups</foreign_table>
                        </config>
                    </TCEforms>
                </fe_groups>
              
                <fe_users>
                    <TCEforms type="array">
                        <label>Välj en eller flera användare</label>
                        <config>
                            <type>select</type>
                            <size>10</size>
                            <maxitems>300</maxitems>
                            <items type="array">
                                <numIndex index="0" type="array">
                                    <numIndex index="0"></numIndex>
                                    <numIndex index="1"></numIndex>
                                </numIndex>
                            </items>
                            <foreign_table>fe_users</foreign_table>
                            <foreign_table_where>ORDER BY username ASC</foreign_table_where>
                        </config>
                    </TCEforms>
                </fe_users>
                
                <!--  <fe_users>
                    <TCEforms type="array">
                     <label>Välj en användare</label>
                     <config>
                       <type>select</type>
                       <size>1</size>
                       <maxitems>1</maxitems>
                       <items type="array">
                         <numIndex index="0" type="array">
                           <numIndex index="0"></numIndex>
                           <numIndex index="1"></numIndex>
                         </numIndex>
                       </items>
                       <foreign_table>fe_users</foreign_table>
                       <foreign_table_where>ORDER BY username ASC</foreign_table_where>
                     </config>
                    </TCEforms>
                </fe_users> 
            
                <fe_users>
                    <TCEforms>
                       <label>Lägg till personer</label>
                        <config>
                            <type>user</type>
                            <userFunc>user_sampleflex_addFieldsToFlexForm->addPeople</userFunc>
                        </config>
                    </TCEforms>
                </fe_users>-->
            
                <!--<projects>
                    <TCEforms>
                        <label>Välj ett/flera forskningsprojekt</label>
                        <config>
                            <type>user</type>
                            <userFunc>user_sampleflex_addFieldsToFlexForm->getProjects</userFunc>
                        </config>
                         <config>
                        <type>select</type>
                         <itemsProcFunc>user_sampleflex_addFieldsToFlexForm->getProjects</itemsProcFunc>
                       </config>
                    </TCEforms>
                </projects> -->
            
                <staffDetailPage>
                    <TCEforms>
                       <label>Page with STAFF details (in detail view only)</label>
                       <config>
                           <type>group</type>
                           <internal_type>db</internal_type>
                           <allowed>pages</allowed>
                           <size>1</size>
                           <maxitems>1</maxitems>
                           <minitems>0</minitems>
                           <show_thumbs>0</show_thumbs>
                       </config>
                    </TCEforms>
                </staffDetailPage>
            
                <projectDetailPage>
                    <TCEforms>
                       <label>Page with PROJECT details (in detail view only)</label>
                       <config>
                           <type>group</type>
                           <internal_type>db</internal_type>
                           <allowed>pages</allowed>
                           <size>1</size>
                           <maxitems>1</maxitems>
                           <minitems>0</minitems>
                           <show_thumbs>0</show_thumbs>
                       </config>
                    </TCEforms>
                </projectDetailPage>

                <categories>
                    <TCEforms>
                        <label>Kategorivisningsläge</label>
                        <config>
                            <default>no_categories</default>
                            <type>radio</type>
                               <items type="array">
                                 <numIndex index="0" type="array">
                                   <numIndex index="0">Inga kategorier</numIndex>
                                   <numIndex index="1">no_categories</numIndex>
                                 </numIndex>
                                 <numIndex index="1" type="array">
                                   <numIndex index="0">Standardkategorier</numIndex>
                                   <numIndex index="1">standard_category</numIndex>
                                 </numIndex>
                               </items>
                        </config>
                    </TCEforms>
                </categories>
            
                <noItemsToShow>
                    <TCEforms>
                        <label>No of items to show</label>
                        <config>
                            <type>select</type>
                            <items type="array">
                                <numIndex index="0" type="array">
                                <numIndex index="0">10</numIndex>
                                <numIndex index="1">10</numIndex>
                                </numIndex>
                                <numIndex index="1" type="array">
                                <numIndex index="0">25</numIndex>
                                <numIndex index="1">25</numIndex>
                                </numIndex>
                                <numIndex index="2" type="array">
                                <numIndex index="0">50</numIndex>
                                <numIndex index="1">50</numIndex>
                                </numIndex>
                                <numIndex index="3" type="array">
                                <numIndex index="0">All</numIndex>
                                <numIndex index="1">15000</numIndex>
                                </numIndex>
                            </items>
                            <maxitems>1</maxitems>
                            <size>1</size>
                            <default>25</default>
                            <selectedListStyle>width:200px</selectedListStyle>
                            <itemListStyle>width:200px</itemListStyle>
                        </config>
                    </TCEforms>
                </noItemsToShow>
            </el>
        </ROOT>
    </sDEF>

    <managePublications>
        <ROOT>
            <TCEforms>
                <sheetTitle>Manage Publications</sheetTitle>
            </TCEforms>
            <el>
                <dynField>
                    <TCEforms>
                        <config>
                            <type>user</type>
                            <userFunc>user_sampleflex_addFieldsToFlexForm->managePublications</userFunc>
                        </config>
                    </TCEforms>
                </dynField>
            </el>
        </ROOT>
    </managePublications>

    </sheets>
</T3DataStructure>