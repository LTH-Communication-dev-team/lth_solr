<T3DataStructure>
 <meta>
       <langDisable>1</langDisable>
 </meta>
<sheets>
  <sDEF>
        <ROOT>
         <TCEforms>
          <sheetTitle>General</sheetTitle>
         </TCEforms>
         <type>array</type>
         <el>
             
            <showStaff>
                <TCEforms>
                    <label>Visa</label>
                    <config>
                        <type>check</type>
                        <items type="array">
                            <numIndex index="0" type="array">
                                    <numIndex index="0">Show staff</numIndex>
                            </numIndex>
                        </items> 
                    </config>
                </TCEforms>
            </showStaff>
            <showPublications>
                <TCEforms>
                    <config>
                        <type>check</type>
                        <items type="array">
                            <numIndex index="0" type="array">
                                    <numIndex index="0">Show publications</numIndex>
                            </numIndex>
                        </items> 
                    </config>
                </TCEforms>
            </showPublications>
            <showProjects>
                <TCEforms>
                    <config>
                        <type>check</type>
                        <items type="array">
                            <numIndex index="0" type="array">
                                    <numIndex index="0">Show projects</numIndex>
                            </numIndex>
                        </items> 
                    </config>
                </TCEforms>
            </showProjects>
            <fe_users>
                <TCEforms type="array">
                 <label>Välj en användare</label>
                 <config>
                   <type>select</type>
                   <size>1</size>
                   <maxitems>1</maxitems>
                   <items type="array">
                     <numIndex index="0" type="array">
                       <numIndex index="0"></numIndex>
                       <numIndex index="1"></numIndex>
                     </numIndex>
                   </items>
                   <foreign_table>fe_users</foreign_table>
                   <foreign_table_where>ORDER BY username ASC</foreign_table_where>
                 </config>
                </TCEforms>
            </fe_users>
            
            <publicationDetailPage>
                <TCEforms>
                   <label>Page with publication details</label>
                   <config>
                       <type>group</type>
                       <internal_type>db</internal_type>
                       <allowed>pages</allowed>
                       <size>1</size>
                       <maxitems>1</maxitems>
                       <minitems>0</minitems>
                       <show_thumbs>0</show_thumbs>
                   </config>
                </TCEforms>
            </publicationDetailPage>
            
            <projectDetailPage>
                <TCEforms>
                   <label>Page with project details</label>
                   <config>
                       <type>group</type>
                       <internal_type>db</internal_type>
                       <allowed>pages</allowed>
                       <size>1</size>
                       <maxitems>1</maxitems>
                       <minitems>0</minitems>
                       <show_thumbs>0</show_thumbs>
                   </config>
                </TCEforms>
            </projectDetailPage>
            
            <noItemsToShow>
                <TCEforms>
                    <label>No of items to show</label>
                    <config>
                    <type>select</type>
                    <items type="array">
                    <numIndex index="0" type="array">
                    <numIndex index="0">10</numIndex>
                    <numIndex index="1">10</numIndex>
                    </numIndex>
                    <numIndex index="1" type="array">
                    <numIndex index="0">25</numIndex>
                    <numIndex index="1">25</numIndex>
                    </numIndex>
                    <numIndex index="2" type="array">
                    <numIndex index="0">50</numIndex>
                    <numIndex index="1">50</numIndex>
                    </numIndex>
                    <numIndex index="3" type="array">
                    <numIndex index="0">All</numIndex>
                    <numIndex index="1">-1</numIndex>
                    </numIndex>
                    </items>
                    <maxitems>1</maxitems>
                    <size>1</size>
                    <default>25</default>
                    <selectedListStyle>width:200px</selectedListStyle>
                    <itemListStyle>width:200px</itemListStyle>
                    </config>
                </TCEforms>
         </noItemsToShow>
         </el>
     </ROOT>
</sDEF>

</sheets>
</T3DataStructure>