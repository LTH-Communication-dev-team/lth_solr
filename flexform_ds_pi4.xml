<T3DataStructure>
    <meta>
       <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>General</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <fe_groups>
                        <TCEforms type="array">
                            <label>FE Groups</label>
                            <config>
                                <type>select</type>
                                <maxitems>100</maxitems>
                                <size>10</size>
                                <itemsProcFunc>user_sampleflex_addFieldsToFlexForm->getOrganisations</itemsProcFunc>
                                <selectedListStyle>width:250px</selectedListStyle>
                                <itemListStyle>width:250px</itemListStyle>
                                <enableMultiSelectFilterTextfield>true</enableMultiSelectFilterTextfield>
                            </config>
                        </TCEforms>
                    </fe_groups>
                    <noItemsToShow>
                        <TCEforms>
                            <label>No of items to show</label>
                            <config>
                            <type>select</type>
                            <items type="array">
                            <numIndex index="0" type="array">
                            <numIndex index="0">10</numIndex>
                            <numIndex index="1">10</numIndex>
                            </numIndex>
                            <numIndex index="1" type="array">
                            <numIndex index="0">25</numIndex>
                            <numIndex index="1">25</numIndex>
                            </numIndex>
                            <numIndex index="2" type="array">
                            <numIndex index="0">50</numIndex>
                            <numIndex index="1">50</numIndex>
                            </numIndex>
                            <numIndex index="3" type="array">
                            <numIndex index="0">All</numIndex>
                            <numIndex index="1">-1</numIndex>
                            </numIndex>
                            </items>
                            <maxitems>1</maxitems>
                            <size>1</size>
                            <default>25</default>
                            <selectedListStyle>width:200px</selectedListStyle>
                            <itemListStyle>width:200px</itemListStyle>
                            </config>
                        </TCEforms>
                    </noItemsToShow>
                    <publicationLink>
                        <TCEforms>
                            <label>Link to display publication</label>
                            <config>
                                <type>text</type>
                                <cols>24</cols>
                                <rows>3</rows>
                            </config>
                        </TCEforms>
                    </publicationLink>
                    <peopleLink>
                        <TCEforms>
                            <label>Link to display people</label>
                            <config>
                                <type>text</type>
                                <cols>24</cols>
                                <rows>3</rows>
                            </config>
                        </TCEforms>
                    </peopleLink>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>