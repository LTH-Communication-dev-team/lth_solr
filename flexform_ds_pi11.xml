<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>General</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <display>
                        <TCEforms>
                            <label>What to display</label>
                            <onChange>reload</onChange>
                            <config>
                                <type>radio</type>
                                <default>staff</default>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">Staff</numIndex>
                                        <numIndex index="1">staff</numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">Organisation</numIndex>
                                        <numIndex index="1">organisation</numIndex>
                                    </numIndex>
                                    <numIndex index="2" type="array">
                                        <numIndex index="0">Roles</numIndex>
                                        <numIndex index="1">roles</numIndex>
                                    </numIndex>
                                    <numIndex index="3" type="array">
                                        <numIndex index="0">Publications</numIndex>
                                        <numIndex index="1">publications</numIndex>
                                    </numIndex>
                                    <numIndex index="4" type="array">
                                        <numIndex index="0">Student Papers</numIndex>
                                        <numIndex index="1">studentpapers</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </display>
                    <organisation>
                        <TCEforms>
                            <label>Organisation</label>
                            <config>
                                <type>select</type>
                                <maxitems>10</maxitems>
                                <size>10</size>
                                <itemsProcFunc>user_sampleflex_addFieldsToFlexForm->getOrganisation</itemsProcFunc>
                                <selectedListStyle>width:250px</selectedListStyle>
                                <itemListStyle>width:250px</itemListStyle>
                                <enableMultiSelectFilterTextfield>true</enableMultiSelectFilterTextfield>
                            </config>
                       </TCEforms>
                    </organisation>
                    <vroles>
                        <TCEforms>
                            <label>Roles</label>
                            <displayCond>FIELD:display:=:roles</displayCond>
                            <config>
                                <type>select</type>
                                <maxitems>10</maxitems>
                                <size>10</size>
                                <itemsProcFunc>user_sampleflex_addFieldsToFlexForm->getVroles</itemsProcFunc>
                                <selectedListStyle>width:250px</selectedListStyle>
                                <itemListStyle>width:250px</itemListStyle>
                                <enableMultiSelectFilterTextfield>true</enableMultiSelectFilterTextfield>
                            </config>
                       </TCEforms>
                    </vroles>
                    <facetChoice>
                        <TCEforms>
                            <label>Facets</label>
                            <config>
                                <type>radio</type>
                                <default>standardCategory</default>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">Standard</numIndex>
                                        <numIndex index="1">standardCategory</numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">Organisation</numIndex>
                                        <numIndex index="1">primaryVroleOu</numIndex>
                                    </numIndex>
                                    <numIndex index="2" type="array">
                                        <numIndex index="0">First Letter</numIndex>
                                        <numIndex index="1">firstLetter</numIndex>
                                    </numIndex>
                                    <numIndex index="3" type="array">
                                        <numIndex index="0">Inga</numIndex>
                                        <numIndex index="1"></numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </facetChoice>
                    <hideSearchField>
                        <TCEforms>
                            <label>Hide Search Field</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </hideSearchField>
                    <extraPeople>
                        <section>1</section>
                        <title>Add people</title>
                        <type>array</type>
                        <el>
                            <container>
                                <title>Add</title>
                                <type>array</type>
                                <el>
                                    <email>
                                        <TCEforms>
                                            <label>Email (for LU-staff just fill in the this)</label>
                                            <config>
                                                <type>input</type>
                                                <size>48</size>
                                            </config>
                                        </TCEforms>
                                    </email>
                                    <title>
                                        <TCEforms>
                                            <label>Title</label>
                                            <config>
                                                <type>text</type>
                                                <cols>24</cols>
                                                <rows>3</rows>
                                            </config>
                                        </TCEforms>
                                    </title>
                                    <name>
                                        <TCEforms>
                                            <label>Name</label>
                                            <config>
                                                <type>input</type>
                                                <size>48</size>
                                            </config>
                                        </TCEforms>
                                    </name>
                                    <organisation>
                                        <TCEforms>
                                            <label>Organisation</label>
                                            <config>
                                                <type>input</type>
                                                <size>48</size>
                                            </config>
                                        </TCEforms>
                                    </organisation>
                                    <hideOrganisation>
                                        <TCEforms>
                                            <label>Hide organisation</label>
                                            <config>
                                                <type>check</type>
                                            </config>
                                        </TCEforms>
                                    </hideOrganisation>                          
                                    <phone>
                                        <TCEforms>
                                            <label>Phone</label>
                                            <config>
                                                <type>input</type>
                                                <size>48</size>
                                            </config>
                                        </TCEforms>
                                    </phone>
                                    <homepage>
                                        <TCEforms>
                                            <label>Homepage</label>
                                            <config>
                                                <type>input</type>
                                                <size>48</size>
                                            </config>
                                        </TCEforms>
                                    </homepage>
                                    <photo>
                                        <TCEforms>
                                            <label>Photo</label>
                                            <config>
                                                <type>group</type>
                                                <internal_type>db</internal_type>
                                                <appearance>
                                                    <elementBrowserType>file</elementBrowserType>
                                                    <elementBrowserAllowed>jpg,jpeg,png,gif</elementBrowserAllowed>
                                                </appearance>
                                                <allowed>sys_file</allowed>
                                                <size>1</size>
                                                <minitems>0</minitems>
                                                <maxitems>1</maxitems>
                                                <show_thumbs>1</show_thumbs>

                                                <dce_load_schema>1</dce_load_schema>
                                                <dce_get_fal_objects>1</dce_get_fal_objects>
                                            </config>
                                        </TCEforms>
                                    </photo>
                                </el>
                            </container>
                        </el>
                    </extraPeople>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>