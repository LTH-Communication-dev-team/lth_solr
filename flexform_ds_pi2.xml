         <detailpage>
             <TCEforms>
                <label>Page with details</label>
                <config>
                    <type>group</type>
                    <internal_type>db</internal_type>
                    <allowed>pages</allowed>
                    <size>1</size>
                    <maxitems>1</maxitems>
                    <minitems>0</minitems>
                    <show_thumbs>0</show_thumbs>
                </config>
             </TCEforms>
         </detailpage>
         
         <scope>
             <TCEforms>
                <label>Urval på institutioner och avdelningar (en per rad)</label>
                <config>
                <type>text</type>
                <cols>50</cols>
               <rows>5</rows>
                </config>
             </TCEforms>
         </scope>

         <addpeople>
             <TCEforms>
                <label>Lägg till personer (en per rad)</label>
                <config>
                    <type>text</type>
                    <cols>50</cols>
                    <rows>5</rows>
                </config>
             </TCEforms>
         </addpeople>

         <removepeople>
             <TCEforms>
             <label>Ta bort personer (en per rad)</label>
                    <config>
                        <type>text</type>
                        <cols>50</cols>
                        <rows>5</rows>
                    </config>
             </TCEforms>
         </removepeople>
         
         <categories>
             <TCEforms>
                 <onChange>reload</onChange>
                 <label>Kategorivisningsläge</label>
                 <config>
                     <default>no_categories</default>
                     <type>radio</type>
                        <items type="array">
                          <numIndex index="0" type="array">
                            <numIndex index="0">Inga kategorier</numIndex>
                            <numIndex index="1">no_categories</numIndex>
                          </numIndex>
                          <numIndex index="1" type="array">
                            <numIndex index="0">Standardkategorier</numIndex>
                            <numIndex index="1">standard_category</numIndex>
                          </numIndex>
                          <numIndex index="2" type="array">
                            <numIndex index="0">Egna kategorier</numIndex>
                            <numIndex index="1">custom_category</numIndex>
                          </numIndex>
                        </items>
                 </config>
             </TCEforms>
         </categories>
         
         <customcategories>
             <TCEforms>
                <displayCond>FIELD:categories:=:custom_category</displayCond>
		<label>Egna personalkategorier (en per rad)</label>
		<config>
                    <type>text</type>
                    <cols>50</cols>
                    <rows>5</rows>
		</config>
            </TCEforms>
        </customcategories>

         <hide_search>
             <TCEforms>
                <label>Göm sökrutan</label>
                <config>
                <type>check</type>
                </config>
             </TCEforms>
        </hide_search>
         </el>
     </ROOT>
</sDEF>


<manageStaffList>
    <ROOT>
         <TCEforms>
          <sheetTitle>Manage Staff List</sheetTitle>
         </TCEforms>
        <el>
            <dynField>
            <TCEforms>
                <config>
                 <type>user</type>
                  <userFunc>user_sampleflex_addFieldsToFlexForm->manageStaffList</userFunc>
                </config>
             </TCEforms>
          </dynField>
        </el>
    </ROOT>
</manageStaffList>

</sheets>
</T3DataStructure>